services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: MySecret_9000
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1434
      target: 1433
  demo.apidesign.cache:
    build:
      context: C:\Users\scleynen\source\repos\Demo.ApiDesign
      dockerfile: Demo.ApiDesign.Cache/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Demo.ApiDesign.Cache
      target: base
    depends_on:
    - db
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44374'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: demoapidesigncache:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.2/Demo.ApiDesign.Cache.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 12956
      target: 80
    - published: 44374
      target: 443
    tty: true
    volumes:
    - C:\Users\scleynen\source\repos\Demo.ApiDesign\Demo.ApiDesign.Cache:/app:rw
    - C:\Users\scleynen\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\scleynen\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\scleynen\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\scleynen\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\scleynen\source\repos\Demo.ApiDesign:/src:rw
version: '3.4'